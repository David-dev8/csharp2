<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Quiz_Royale</RootNamespace>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Castle.Windsor" Version="5.1.2" />
    <None Remove="Assets\background.png" />
    <None Remove="Assets\home.png" />
    <None Remove="Assets\profile.png" />
    <None Remove="Assets\shop.png" />
    <None Remove="Assets\signOut.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\background.png" />
    <Resource Include="Assets\home.png">
      <CopyToOutputDirectory></CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\profile.png" />
    <Resource Include="Assets\shop.png" />
    <Resource Include="Assets\signOut.png" />
  </ItemGroup>

</Project>
